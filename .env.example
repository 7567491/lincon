# Linode API Token
# 从 Linode Cloud Manager 获取：https://cloud.linode.com/profile/tokens
VITE_LINODE_API_TOKEN=your_linode_api_token_here

# S3/Object Storage 访问凭证
# 从 Linode Cloud Manager 的 Object Storage Keys 获取
VITE_S3_ACCESS_KEY=your_s3_access_key_here
VITE_S3_SECRET_KEY=your_s3_secret_key_here
VITE_S3_REGION=ap-south-1
VITE_S3_ENDPOINT=https://ap-south-1.linodeobjects.com

# API基础URL (可选，默认为官方API)
VITE_API_BASE_URL=https://api.linode.com/v4